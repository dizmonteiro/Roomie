<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="roomie.entities.Application" table="Application" lazy="false">
		<composite-id>
			<key-many-to-one name="tenant" column="TenantID" class="roomie.entities.Tenant">
			</key-many-to-one>
			<key-many-to-one name="house" column="HouseID" class="roomie.entities.House">
			</key-many-to-one>
		</composite-id>
		<property name="toBeAssessed" column="ToBeAssessed" type="boolean" not-null="true" lazy="false"/>
		<property name="accepted" column="Accepted" type="boolean" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
